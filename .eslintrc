{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "_" : true,
    "angular" : true,
    "memApp" : true,
    "$" : true
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "rules" : {
    "new-cap": 0,
    "no-case-declarations": 0,
    "no-nested-ternary": 0,
    "no-underscore-dangle": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 1
  }
}